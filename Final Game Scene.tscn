[sub_resource type="GDScript" id="GDScript_nq346"]
script/source = "extends Control

# Store the current component placed in this drop zone
var current_component: Node = null

# Called when a component is dropped on the drop zone
func set_component(component: Node) -> void:
	# If a component has already been placed, ignore further drops
	if current_component:
		return
	
	# Store the component that was dropped here
	current_component = component
	# Call the component's light_up() method
	current_component.light_up()
	
	# Optionally: Disable further drops or take other actions after a component is dropped
	set_process_input(false)  # This can be optional depending on your game rules

# Called when the drop zone is cleared (optional)
func clear_zone() -> void:
	if current_component:
		# Reset the component's state (reset the visual effects)
		current_component.reset_component()
		current_component = null
	
	# Re-enable input processing for future drops
	set_process_input(true)
"

